<?xml version="1.0" standalone="yes"?>
<xs:schema id="dsUsers" targetNamespace="http://www.tempuri.org/dsUsers.xsd" xmlns:mstns="http://www.tempuri.org/dsUsers.xsd" xmlns="http://www.tempuri.org/dsUsers.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" attributeFormDefault="qualified" elementFormDefault="qualified">
  <xs:element name="dsUsers" msdata:IsDataSet="true" msdata:Locale="ru-RU">
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element name="Users">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="UserID" msdata:ReadOnly="true" msdata:AutoIncrement="true" type="xs:int" />
              <xs:element name="UserName" type="xs:string" />
              <xs:element name="UserPassword" type="xs:string" />
              <xs:element name="GroupID" type="xs:int" />
              <xs:element name="IsRemoved" type="xs:boolean" />
              <xs:element name="IsAdmin" type="xs:boolean" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:choice>
    </xs:complexType>
    <xs:unique name="Constraint1" msdata:PrimaryKey="true">
      <xs:selector xpath=".//mstns:Users" />
      <xs:field xpath="mstns:UserID" />
    </xs:unique>
  </xs:element>
</xs:schema>