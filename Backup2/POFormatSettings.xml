    </fields>
    <decimalsplitter>.</decimalsplitter>
    <dateformat>
      <datesplitter>/</datesplitter>
      <day position="1" form="short">
      </day>
      <month position="2" form="letters1">
      </month>
      <year position="3" form="short">
      </year>
      <example>Jan/12/1980</example>
    </dateformat>
  </settings>
